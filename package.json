{
    "name": "thyme",
    "version": "1.7.0",
    "private": false,
    "homepage": "/",
    "scripts": {
        "analyze": "source-map-explorer build/static/js/main.*",
        "generate-sample": "node ./scripts/create-mock",
        "start": "react-scripts start",
        "eject": "react-scripts eject",
        "test": "react-scripts test --env=jsdom",
        "test:ci": "CI=true npm run test",
        "lint:css": "stylelint \"./src/**/*.css\"",
        "lint:js": "eslint \"./+(src|scripts)/**/*.js\"",
        "lint": "npm run lint:js && npm run lint:css",
        "flow": "flow",
        "add-sw-code": "cat ./src/custom-sw.js >> ./build/service-worker.js",
        "build": "env-cmd .env.production react-scripts build && npm run add-sw-code && cp package.json build/package.json",
        "build:staging": "env-cmd .env.staging react-scripts build && npm run add-sw-code",
        "build:dev": "NODE_ENV=development webpack --config node_modules/react-scripts/config/webpack.config.dev.js",
        "stats": "NODE_ENV=production webpack --config node_modules/react-scripts/config/webpack.config.prod.js --profile --json > stats.json",
        "extensions": "webpack --config extension/webpack.config.js",
        "zip:chrome": "rm -f extension/chrome/output/archive.zip && zip -r extension/chrome/output/archive.zip extension/chrome -x \"*/output*\" -x \"*.DS_Store\"",
        "build:extensions": "NODE_ENV=production npm run extensions && npm run zip:chrome",
        "start:extensions": "NODE_ENV=development npm run extensions -- --watch"
    },
    "dependencies": {
        "@sentry/browser": "^4.4.2",
        "classnames": "^2.2.6",
        "date-fns": "^1.30.1",
        "env-cmd": "^8.0.2",
        "enzyme": "^3.8.0",
        "enzyme-adapter-react-16": "^1.7.1",
        "file-saver": "^2.0.0",
        "formik": "^1.4.1",
        "left-pad": "^1.3.0",
        "lodash": "^4.17.11",
        "mitt": "^1.1.3",
        "qs": "^6.6.0",
        "react": "^16.6.3",
        "react-chartjs": "^1.2.0",
        "react-dom": "^16.6.3",
        "react-dom-confetti": "0.0.10",
        "react-redux": "^6.0.0",
        "react-router": "^4.3.1",
        "react-router-dom": "^4.3.1",
        "react-scripts": "^2.1.1",
        "react-stripe-elements": "^2.0.1",
        "react-svg-piechart": "^2.1.1",
        "react-test-renderer": "^16.6.3",
        "redux": "^4.0.1",
        "redux-devtools-extension": "^2.13.7",
        "redux-observable": "^1.0.0",
        "reselect": "^4.0.0",
        "rxjs": "^6.3.3",
        "semantic-ui-css": "^2.4.1",
        "semantic-ui-react": "^0.84.0",
        "shortid": "^2.2.14",
        "source-map-explorer": "^1.6.0",
        "thyme-connect": "^1.0.2"
    },
    "devDependencies": {
        "@babel/core": "^7.2.2",
        "@babel/plugin-proposal-class-properties": "^7.2.3",
        "@babel/preset-env": "^7.2.3",
        "@babel/preset-flow": "^7.0.0",
        "@babel/preset-react": "^7.0.0",
        "css-loader": "^2.1.0",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-plugin-flowtype": "^3.2.0",
        "eslint-plugin-import": "^2.15.0",
        "eslint-plugin-jsx-a11y": "^6.1.2",
        "eslint-plugin-react": "^7.12.4",
        "flow-bin": "^0.91.0",
        "pre-push": "^0.1.1",
        "stylelint": "^9.9.0",
        "stylelint-config-standard": "^18.2.0",
        "webpack-cli": "^3.1.2"
    },
    "pre-push": [
        "lint",
        "flow",
        "test:ci",
        "build:dev"
    ],
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ]
}
