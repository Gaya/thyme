{
    "name": "thyme",
    "version": "0.1.0",
    "private": false,
    "homepage": "/",
    "scripts": {
        "analyze": "source-map-explorer build/static/js/main.*",
        "start": "react-scripts start",
        "eject": "react-scripts eject",
        "test": "react-scripts test --env=jsdom",
        "test:ci": "CI=true npm run test",
        "lint:css": "stylelint \"./src/**/*.css\"",
        "lint:js": "eslint ./src",
        "lint": "npm run lint:js && npm run lint:css",
        "flow": "flow",
        "build": "env-cmd .env.production react-scripts build",
        "build:staging": "env-cmd .env.staging react-scripts build",
        "build:dev": "NODE_ENV=development webpack --config node_modules/react-scripts/config/webpack.config.dev.js",
        "stats": "NODE_ENV=production webpack --config node_modules/react-scripts/config/webpack.config.prod.js --profile --json > stats.json"
    },
    "dependencies": {
        "classnames": "^2.2.6",
        "date-fns": "^1.29.0",
        "env-cmd": "^8.0.2",
        "enzyme": "^3.6.0",
        "enzyme-adapter-react-16": "^1.5.0",
        "file-saver": "^1.3.8",
        "left-pad": "^1.3.0",
        "lodash": "^4.17.11",
        "qs": "^6.5.2",
        "react": "^16.5.2",
        "react-dom": "^16.5.2",
        "react-redux": "^5.0.6",
        "react-router": "^4.3.1",
        "react-router-dom": "^4.3.1",
        "react-scripts": "^2.0.3",
        "react-svg-piechart": "^2.1.1",
        "react-test-renderer": "^16.5.2",
        "redux": "^4.0.0",
        "redux-devtools-extension": "^2.13.5",
        "redux-form": "^7.4.2",
        "reselect": "^4.0.0",
        "semantic-ui-css": "^2.4.0",
        "semantic-ui-react": "^0.82.5",
        "shortid": "^2.2.13",
        "source-map-explorer": "^1.6.0"
    },
    "devDependencies": {
        "eslint-config-airbnb": "^17.1.0",
        "eslint-plugin-flowtype": "^2.50.3",
        "eslint-plugin-import": "^2.14.0",
        "eslint-plugin-jsx-a11y": "^6.1.1",
        "eslint-plugin-react": "^7.11.1",
        "flow-bin": "^0.82.0",
        "pre-push": "^0.1.1",
        "stylelint": "^9.6.0",
        "stylelint-config-standard": "^18.2.0",
        "webpack-cli": "^3.1.2"
    },
    "pre-push": [
        "lint",
        "flow",
        "test:ci",
        "build:dev"
    ],
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ]
}
